{
  "name": "@payconductor-sdk-web/library-react",
  "version": "1.0.9",
  "description": "React SDK for PayConductor",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/payconductor-ai/payconductor-sdk-web"
  },
  "bugs": {
    "url": "https://github.com/payconductor-ai/payconductor-sdk-web/issues"
  },
  "homepage": "https://github.com/payconductor-ai/payconductor-sdk-web#readme",
  "license": "MIT",
  "author": "PayConductor",
  "keywords": [
    "payconductor",
    "payment",
    "gateway",
    "react",
    "sdk",
    "checkout"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.0"
  }
}
